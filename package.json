{
  "private": true,
  "license": "GPL-3.0-or-later",
  "version": "0.1.0",
  "name": "my-storyblok-demo",
  "packageManager": "pnpm@10.22.0",
  "type": "module",
  "engines": {
    "node": ">=24 <25",
    "pnpm": ">=10 <11"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3010 --experimental-https --experimental-https-key ./.dev/certs/localhost-key.pem --experimental-https-cert ./.dev/certs/localhost-crt.pem",
    "dev-setup": "node ./.dev/certs/generate-cert.cjs",
    "sb:build": "storyblok build",
    "sb:generate:blok-union": "node ./.dev/storyblok/generate-union-type.ts",
    "sb:login": "storyblok logout && storyblok login --region eu",
    "sb:logout": "storyblok logout",
    "sb:pull": "node ./.dev/storyblok/pull-resources.mjs && npm run sb:generate:blok-union",
    "sb:pull-components": "node ./.dev/storyblok/pull-components.mjs",
    "sb:pull-datasources": "node ./.dev/storyblok/pull-datasources.mjs",
    "sb:pull-languages": "node ./.dev/storyblok/pull-languages.mjs",
    "sb:pull-types": "node ./.dev/storyblok/pull-types.mjs && npm run sb:generate:blok-union",
    "lint:storyblok-slugs": "node ./scripts/check-storyblok-slugs.mjs",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck:dev": "tsc -p ./.dev/tsconfig.json --noEmit",
    "cy:open": "cypress open --config-file tests/e2e/cypress.config.ts",
    "cy:run": "cypress run --config-file tests/e2e/cypress.config.ts",
    "vercel": "vercel env pull"
  },
  "dependencies": {
    "@mantine/core": "^8.3.8",
    "@mantine/hooks": "^8.3.8",
    "@storyblok/js": "^4.4.1",
    "@storyblok/react": "^5.4.18",
    "@tanstack/react-query": "^5.90.16",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "classnames": "^2.5.1",
    "html-react-parser": "^5.2.10",
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.70.0",
    "sass": "^1.94.2",
    "server-only": "^0.0.1",
    "storyblok-js-client": "^7.1.6",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.0.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "cypress": "^13.13.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-mantine": "^4.0.3",
    "eslint-config-next": "^16.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "prettier": "3.6.2",
    "storyblok": "^4.7.0",
    "storyblok-generate-ts": "^2.2.0",
    "typescript": "5.9.3",
    "vitest": "^2.1.4"
  },
  "sideEffects": [
    "*.scss"
  ]
}
