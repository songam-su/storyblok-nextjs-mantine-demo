{
  "private": true,
  "version": "0.1.0",
  "name": "my-storyblok-demo",
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3010 --experimental-https --experimental-https-key ./.dev/certs/localhost-key.pem --experimental-https-cert ./.dev/certs/localhost-crt.pem",
    "dev-setup": "node ./.dev/certs/generate-cert",
    "sb:build": "storyblok build",
    "sb:generate:blok-union": "node ./.dev/storyblok/generate-union-type.ts",
    "sb:login": "storyblok logout && storyblok login --region eu",
    "sb:logout": "storyblok logout",
    "sb:pull": "node ./.dev/storyblok/pull-resources.mjs && npm run sb:generate:blok-union",
    "sb:pull-components": "node ./.dev/storyblok/pull-components.mjs",
    "sb:pull-datasources": "node ./.dev/storyblok/pull-datasources.mjs",
    "sb:pull-languages": "node ./.dev/storyblok/pull-languages.mjs",
    "sb:pull-types": "node ./.dev/storyblok/pull-types.mjs && npm run sb:generate:blok-union",
    "start": "next start",
    "vercel": "vercel env pull"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^8.3.8",
    "@mantine/hooks": "^8.3.8",
    "@mantine/next": "^6.0.22",
    "@storyblok/js": "^4.4.1",
    "@storyblok/react": "^5.4.18",
    "@tanstack/react-query": "^5.90.10",
    "@vercel/speed-insights": "^1.2.0",
    "classnames": "^2.5.1",
    "html-react-parser": "^5.2.10",
    "next": "latest",
    "nuqs": "^2.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "sass": "^1.94.2",
    "search-insights": "^2.17.3",
    "storyblok-js-client": "^7.1.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-mantine": "^4.0.3",
    "eslint-config-next": "^16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "3.6.2",
    "storyblok": "^4.7.0",
    "typescript": "5.9.3"
  },
  "sideEffects": [
    "*.scss"
  ]
}
