{
  "private": true,
  "version": "0.1.0",
  "name": "songam-brand-site",
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3010 --experimental-https --experimental-https-key ./.dev/certs/localhost-key.pem --experimental-https-cert ./.dev/certs/localhost-crt.pem",
    "dev-setup": "node ./.dev/generate-cert",
    "sb:build": "storyblok build",
    "sb:components": "storyblok pull-components --space 288596360272599 --path ./src/lib/storyblok/resources/",
    "sb:datasources": "storyblok datasources pull --space 288596360272599",
    "sb:languages": "storyblok languages pull --space 288596360272599",
    "sb:login": "storyblok logout && storyblok login --region eu",
    "sb:logout": "storyblok logout",
    "sb:types": "pnpm sb:components && storyblok-generate-ts source=./src/lib/storyblok/resources/components.288596360272599.json target=./src/lib/storyblok/resources/bloks.288596360272599",
    "start": "next start",
    "vercel": "vercel env pull"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^8.3.8",
    "@mantine/hooks": "^8.3.8",
    "@mantine/next": "^6.0.22",
    "@storyblok/js": "^4.4.1",
    "@storyblok/react": "^5.4.18",
    "@tanstack/react-query": "^5.90.10",
    "@vercel/speed-insights": "^1.2.0",
    "classnames": "^2.5.1",
    "debug": "^4.4.3",
    "html-react-parser": "^5.2.10",
    "next": "latest",
    "nuqs": "^2.8.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "sass": "^1.94.2",
    "search-insights": "^2.17.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "mkcert": "^3.2.0",
    "prettier": "3.6.2",
    "selfsigned": "^4.0.0",
    "typescript": "5.9.3"
  },
  "sideEffects": [
    "*.scss"
  ]
